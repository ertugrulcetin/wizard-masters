{
  "name": "prototype-game",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "watch": "sh ./sh/copy_havok.sh && npx shadow-cljs watch app",
    "clean": "rm -rf .shadow-cljs resources/public/js/compiled target",
    "clean-all": "rm -rf .shadow-cljs resources/public/js/compiled node_modules package-lock.json target",
    "release": "sh ./sh/copy_havok.sh && npx shadow-cljs release app"
  },
  "devDependencies": {
    "shadow-cljs": "2.28.4"
  },
  "dependencies": {
    "@babylonjs/core": "^7.33.0",
    "@babylonjs/gui": "^7.33.0",
    "@babylonjs/havok": "^1.3.10",
    "@babylonjs/inspector": "^7.33.0",
    "@babylonjs/loaders": "^7.33.0",
    "@babylonjs/materials": "^7.33.0",
    "@sentry/browser": "^8.34.0",
    "@tweenjs/tween.js": "^23.1.2",
    "bad-words": "^4.0.0",
    "earcut": "^2.2.4",
    "lil-gui": "^0.19.2",
    "nipplejs": "^0.10.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  }
}
